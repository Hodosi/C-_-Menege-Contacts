CREATE TABLE [dbo].[Users] (
    [IdUser]     INT          IDENTITY (1, 1) NOT NULL,
    [First Name] VARCHAR (50) NULL,
    [Last Name]  VARCHAR (50) NULL,
    [Username]   VARCHAR (50) NULL,
    [Password]   VARCHAR (50) NULL,
    [Picture]    IMAGE        NULL,
    PRIMARY KEY CLUSTERED ([IdUser] ASC)
);

